<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.admin.app"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.RegisterActivity"/>
        <activity android:name=".activity.Register2Activity"/>
        <activity android:name=".activity.ChangePasswordActivity"/>
        <activity android:name=".activity.ChangePassword2Activity"/>
        <activity android:name=".activity.ChangePassword3Activity"/>
        <activity android:name=".activity.CategoryActivity"/>
        <activity android:name=".activity.SubclassActivity"/>
        <activity android:name=".activity.SettingActivity"/>
    </application>

</manifest>
